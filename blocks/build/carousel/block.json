{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "crosswinds-blocks/carousel",
  "version": "1.0",
  "title": "Carousel",
  "category": "crosswinds-blocks",
  "description": "Add a custom carousel to your content.",
  "attributes": {
    "arrowsLocation": {
      "type": "string",
      "default": "bottom-right-corner"
    },
    "showDots": {
      "type": "boolean",
      "default": false
    },
    "arrowsColor": {
      "type": "string"
    },
    "arrowsBackgroundColor": {
      "type": "string"
    },
    "arrowsHoverColor": {
      "type": "string"
    },
    "arrowsHoverBackgroundColor": {
      "type": "string"
    },
    "arrowsColorValue": {
      "type": "string"
    },
    "arrowsBackgroundColorValue": {
      "type": "string"
    },
    "arrowsHoverColorValue": {
      "type": "string"
    },
    "arrowsHoverBackgroundColorValue": {
      "type": "string"
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false,
    "ariaLabel": true,
    "__experimentalLayout": true,
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ],
      "padding": true,
      "blockGap": true,
      "__experimentalDefaultControls": {
        "padding": true,
        "blockGap": true
      }
    },
    "shadow": true,
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "color": true,
        "radius": true,
        "style": true,
        "width": true
      }
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalDefaultControls": {
        "fontSize": true
      }
    }
  },
  "textdomain": "crosswinds",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}